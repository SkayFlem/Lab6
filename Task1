#include <stdio.h>

int main() {
    // a. Объявление целой переменной year
    int year;
    
    // b. Приглашение к вводу года
    printf("Введите год: ");
    
    // c. Ввод с клавиатуры значения
    scanf("%d", &year);
    
    // d. Вычисление остатков от деления
    int remainder4 = year % 4;
    int remainder100 = year % 100;
    int remainder400 = year % 400;
    
    // Проверка условия високосного года
    if ((remainder4 == 0 && remainder100 != 0) || (remainder400 == 0)) {
        printf("%d год - ВИСОКОСНЫЙ\n", year);
    } else {
        printf("%d год - НЕ ВИСОКОСНЫЙ\n", year);
    }
    
    return 0;
}
